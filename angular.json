{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "core": {
            "projectType": "library",
            "schematics": {
                "@schematics/angular:component": {
                    "changeDetection": "OnPush",
                    "inlineStyle": false,
                    "inlineTemplate": false,
                    "prefix": "ge",
                    "style": "scss"
                },
                "@schematics/angular:directive": {
                    "prefix": "ge"
                }
            },
            "root": "projects/core",
            "sourceRoot": "projects/core/src",
            "prefix": "ge",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "tsConfig": "projects/core/tsconfig.lib.json",
                        "project": "projects/core/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/core/tsconfig.lib.prod.json"
                        },
                        "development": {}
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/core/src/test.ts",
                        "tsConfig": "projects/core/tsconfig.spec.json",
                        "karmaConfig": "projects/core/karma.conf.js"
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["projects/core/**/*.ts", "projects/core/**/*.html"]
                    }
                }
            }
        },
        "wave": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "changeDetection": "OnPush",
                    "inlineStyle": false,
                    "inlineTemplate": false,
                    "prefix": "ge",
                    "style": "scss"
                },
                "@schematics/angular:directive": {
                    "prefix": "ge"
                }
            },
            "root": "projects/wave",
            "sourceRoot": "projects/wave/src",
            "prefix": "ge",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "allowedCommonJsDependencies": [
                            "codemirror",
                            "dagre-d3",
                            "moment",
                            "xml-utils",
                            "lru-cache",
                            "web-worker",
                            "rbush",
                            "clone",
                            "fast-deep-equal",
                            "fast-json-stable-stringify"
                        ],
                        "outputPath": "dist/wave",
                        "index": "projects/wave/src/index.html",
                        "main": "projects/wave/src/main.ts",
                        "polyfills": "projects/wave/src/polyfills.ts",
                        "tsConfig": "projects/wave/tsconfig.app.json",
                        "assets": [
                            "projects/wave/src/favicon.ico",
                            "projects/wave/src/assets",
                            {
                                "glob": "**/*",
                                "input": "dist/core/assets",
                                "output": "assets"
                            }
                        ],
                        "styles": [
                            "projects/wave/src/styles.scss",
                            "dist/core/assets/fonts/roboto/roboto.css",
                            "dist/core/assets/fonts/pacifico/pacifico.css",
                            "dist/core/assets/fonts/material-design-icons/material-icons.css",
                            "node_modules/codemirror/lib/codemirror.css",
                            "node_modules/ol/ol.css",
                            "./node_modules/vis-timeline/dist/vis-timeline-graph2d.min.css"
                        ],
                        "scripts": ["node_modules/vis-timeline/peer/esm/vis-timeline-graph2d.min.js"],
                        "vendorChunk": true,
                        "extractLicenses": false,
                        "buildOptimizer": false,
                        "sourceMap": true,
                        "optimization": false,
                        "namedChunks": true
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "projects/wave/src/environments/environment.ts",
                                    "with": "projects/wave/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "10mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        },
                        "development": {}
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {},
                    "configurations": {
                        "production": {
                            "browserTarget": "wave:build:production"
                        },
                        "development": {
                            "browserTarget": "wave:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "wave:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/wave/src/test.ts",
                        "polyfills": "projects/wave/src/polyfills.ts",
                        "tsConfig": "projects/wave/tsconfig.spec.json",
                        "karmaConfig": "projects/wave/karma.conf.js",
                        "assets": ["projects/wave/src/favicon.ico", "projects/wave/src/assets"],
                        "styles": ["projects/wave/src/styles.scss"],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "options": {
                        "lintFilePatterns": ["projects/wave/**/*.ts", "projects/wave/**/*.html"]
                    }
                },
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "projects/wave/e2e/protractor.conf.js"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "wave:serve:production"
                        },
                        "development": {
                            "devServerTarget": "wave:serve:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        },
